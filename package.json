{
  "name": "shopify_subscriptions_app",
  "version": "1.0.0",
  "description": "A Node.js web server that powers a fully-customized subscription management portal for ReCharge/Shopify customers.",
  "main": "index.js",
  "scripts": {
    "tslint": "tslint -c tslint.json 'src/**/*'",
    "test": "jest",
    "migrate": "knex migrate:latest",
    "tsc": "tsc",
    "webpack": "webpack --config webpack/index.js",
    "webpack-dev-server": "webpack-dev-server --config webpack/index.js",
    "nodemon": "nodemon -r dotenv/config dist/server/index.js",
    "ngrok": "node -r dotenv/config ngrok.js",
    "dev": "run-p \"tsc -- --watch\" webpack-dev-server nodemon ngrok",
    "prestart": "run-s migrate tsc webpack",
    "start": "node dist/server/index.js"
  },
  "author": "Ethan Jon",
  "license": "UNLICENSED",
  "dependencies": {
    "@types/grid-styled": "^4.1.0",
    "@types/jquery": "^3.3.1",
    "@types/koa": "^2.0.45",
    "@types/node": "^9.6.6",
    "@types/react": "^16.3.12",
    "@types/react-dom": "^16.0.5",
    "@types/slick-carousel": "^1.6.32",
    "@types/webpack-env": "^1.13.6",
    "accounting": "^0.4.1",
    "atob": "^2.1.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "date-fns": "^1.29.0",
    "grid-styled": "^4.1.0",
    "knex": "^0.14.6",
    "koa": "^2.5.0",
    "koa-bodyparser": "^4.2.0",
    "koa-logger": "^3.2.0",
    "koa-mount": "^3.0.0",
    "koa-router": "^7.4.0",
    "koa-send": "^4.1.3",
    "mysql2": "^1.5.3",
    "node-fetch": "^2.1.2",
    "npm-run-all": "^4.1.2",
    "raven": "^2.6.3",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-modal": "^3.4.4",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-stickynode": "^1.4.1",
    "rebass": "^2.0.0",
    "shopify-api-node": "^2.14.0",
    "smoothscroll-polyfill": "^0.4.3",
    "styled-components": "^3.2.6",
    "ts-loader": "^4.3.0",
    "typescript": "^2.8.3",
    "webpack": "^4.6.0",
    "webpack-bundle-analyzer": "^2.12.0",
    "webpack-cli": "^2.0.15"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "dotenv": "^5.0.1",
    "jest": "^22.4.3",
    "ngrok": "^3.0.1",
    "nodemon": "^1.17.3",
    "react-hot-loader": "^4.1.3",
    "tslint": "^5.10.0",
    "tslint-react": "^3.6.0",
    "webpack-dev-server": "^3.1.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/jest-preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/*.+(ts|tsx|js)"
    ]
  },
  "sideEffects": false
}

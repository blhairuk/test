{
  "name": "shopify_subscriptions_app",
  "version": "1.0.0",
  "description": "A Node.js web server that powers a fully-customized subscription management portal for ReCharge/Shopify customers.",
  "main": "index.js",
  "scripts": {
    "migrate": "knex migrate:latest",
    "tsc": "tsc",
    "webpack": "webpack",
    "nodemon": "nodemon -r dotenv/config dist/server/index.js",
    "dev": "npm-run-all tsc webpack --parallel \"tsc -- --watch\" \"webpack -- --watch\" nodemon",
    "prestart": "run-s migrate tsc webpack",
    "start": "node dist/server/index.js"
  },
  "author": "Ethan Jon",
  "license": "UNLICENSED",
  "dependencies": {
    "@types/jquery": "^3.3.1",
    "@types/koa": "^2.0.45",
    "@types/node": "^9.6.6",
    "@types/react": "^16.3.12",
    "@types/react-dom": "^16.0.5",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "knex": "^0.14.6",
    "koa": "^2.5.0",
    "koa-logger": "^3.2.0",
    "koa-mount": "^3.0.0",
    "koa-router": "^7.4.0",
    "koa-send": "^4.1.3",
    "mysql2": "^1.5.3",
    "node-fetch": "^2.1.2",
    "npm-run-all": "^4.1.2",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "shopify-api-node": "^2.13.0",
    "styled-components": "^3.2.6",
    "typescript": "^2.8.3",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15"
  },
  "devDependencies": {
    "dotenv": "^5.0.1",
    "nodemon": "^1.17.3"
  }
}

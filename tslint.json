{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest",
    "tslint-react"
  ],
  "jsRules": {},
  "rules": {
    "interface-name": false,
    "jsx-no-multiline-js": false,
    "no-implicit-dependencies": [true, "dev"],
    "semicolon": false,
    "variable-name": [true, "allow-snake-case", "allow-pascal-case"]
  },
  "rulesDirectory": []
}